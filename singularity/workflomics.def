Bootstrap: docker
From: workflomics/base:v1.0
Stage: build


%labels
    Author Nauman Ahmed
    Version 1.0.0
    Container Workflomics
%files
   # Copy the install and test scripts to /data
   install /data

%post

   chmod +x /data/install/*.sh



   # Install the tools
   for file in /data/install/install_*.sh;do $file; done


   rm -rf /data/install
    
   
   

   
    
